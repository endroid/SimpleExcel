{
    "name": "endroid/simple-spreadsheet",
    "description": "Endroid Simple Spreadsheet",
    "keywords": ["endroid", "simple", "spreadsheet", "php"],
    "homepage": "https://github.com/endroid/simple-spreadsheet",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/installer": "^1.0.3",
        "phpoffice/phpspreadsheet": "^1.1",
        "symfony/http-foundation": "^3.4||^4.0||^5.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SimpleSpreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SimpleSpreadsheet\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
