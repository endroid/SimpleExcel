{
    "name": "endroid/simple-excel",
    "description": "Endroid Simple Excel",
    "keywords": ["endroid", "simple", "excel", "php"],
    "homepage": "https://github.com/endroid/simple-excel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/installer": "^1.0.3",
        "phpoffice/phpspreadsheet": "^1.1",
        "symfony/http-foundation": "^3.4||^4.0"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SimpleExcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SimpleExcel\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
